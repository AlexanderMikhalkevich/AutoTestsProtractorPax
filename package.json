{
    "name": "tests_acceptance",
    "private": true,
    "version": "1.0.0",
    "author": "Paxful",
    "description": "Test acceptance package. Used in selenuim, postman and k6 performance tests",
    "scripts": {
        "postinstall": "webdriver-manager update --gecko false $CHROME_VERSION",
        "clean": "rm -rf build",
        "compile": "tsc",
        "test": "yarn compile && protractor build/protractor.conf.js",
        "test:gitlab": "yarn compile && xvfb-run protractor build/protractor.conf.js --profile gitlab",
        "test:postman": "newman run api/CI.postman_collection.json -k -e api/CI.postman_environment.json",
        "test:performance": "node k6-gitlab.js base",
        "lint": "tslint -p ./tsconfig.json"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint"
        }
    },
    "engines": {
        "node": "^12.16"
    },
    "dependencies": {
        "@types/faker": "^4.1.12",
        "@types/jasmine": "^3.5.12",
        "@types/node": "^11.15.20",
        "@types/request-promise-native": "^1.0.15",
        "authenticator-browser-extension": "1.2.0",
        "faker": "^4.1.0",
        "husky": "^3.0.8",
        "jasmine-allure-reporter": "^1.0.2",
        "jasmine-fail-fast": "^2.0.0",
        "k6-to-junit": "^1.0.3",
        "lodash": "^4.17.19",
        "minimist": "^1.2.0",
        "newman": "^4.5.5",
        "protractor": "^7.0.0",
        "request": "^2.88.0",
        "request-promise-native": "^1.0.9",
        "testrail-api": "^1.3.5",
        "tslint": "^5.20.0",
        "tslint-defocus": "^2.0.6",
        "typescript": "^4.0.2",
        "webdriver-manager": "^12.1.7"
    }
}
